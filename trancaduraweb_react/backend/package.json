{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend API with Node.js and TypeScript",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "dev:watch": "nodemon --watch src --ext ts --exec ts-node src/server.ts",
    "clean": "rmdir /s /q dist",
